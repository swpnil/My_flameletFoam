FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphaSgs;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    AXIS
    {
        type            empty;
    }
    OUTLET
    {
        type            calculated;
        value           uniform 0;
    }
    COFLOW
    {
        type            calculated;
        value           uniform 0;
    }
    PILOT
    {
        type            calculated;
        value           uniform 0;
    }
    FUEL
    {
        type            calculated;
        value           uniform 0;
    }
    WALL
    {
        type            zeroGradient;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}


