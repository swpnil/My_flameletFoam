FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      chi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    OUTLET
    {
        type            zeroGradient;
    }
    COFLOW
    {
        type            calculated;
        value           uniform 0;
    }
    PILOT
    {
        type            calculated;
        value           uniform 0;
    }
    FUEL
    {
        type            calculated;
        value           uniform 0;
    }
    SIDES
    {
        type            zeroGradient;
    }
    
}


